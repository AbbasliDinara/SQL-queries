--Customer cədvəlində müştərilərin doğum tarixindən istifadə etməklə doğum illərini göstərin (Məs. 01/01/1990 --> 1990)


select year(cr.birth_date), cr.birth_date
from customer;


select to_char(cr.birth_date,'yyyy'), cr.birth_date
from customer;


select po.PPSTART,to_char(po.PPSTART,'yyyy') from poscoupons po;
