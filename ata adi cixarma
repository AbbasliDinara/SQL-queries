--ata adi cixarma

select AUSDESC,
case when instr(AUSDESC,' ',1,3)>0 and instr(AUSDESC,' ',1,2)>0then

substr(AUSDESC,instr(AUSDESC,' ',1,2),
instr(AUSDESC,' ',1,3) -instr(AUSDESC,' ',1,2))  
when instr(AUSDESC,' ',1,2)>0then


substr(AUSDESC,instr(AUSDESC,' ',1,2),
length(AUSDESC)+1 -instr(AUSDESC,' ',1,2))
else 
null
end as ata_adi
from adm_users
